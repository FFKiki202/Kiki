$datei = './text/feld_freeze_info_3.txt';
@chmod( $datei, 0777 ); //0777 0666
clearstatcache ();

$chmod = decoct( fileperms ( $datei ) );

// echo 'CHMOD nachher: ' . $chmod; 
$dateinamen = './text/feld_freeze_info_3.txt';
