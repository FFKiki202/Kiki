/**  memory text block 1 START **/

$dateinamen_1 = './text/feld_freeze_info_1.txt';
 $handle_1 = fopen ($dateinamen_1, "r");
$inhalt_1 = fread ($handle_1, filesize ($dateinamen_1));
fclose ($handle_1);

if($inhalt_1=="yes"){
	
$datei="./text/feld_1.txt"; 
$array= file($datei); 
$array2= file($datei); 

foreach ($array as $txt4[]){}
$sorta_1=array();
$sorta_1[0]=$txt4[0];
$sorta_1[1]=$txt4[1];
$sorta_1[2]=$txt4[2];
$sorta_1[3]=$txt4[3];
$sorta_1[4]=$txt4[4];


sort($sorta_1,1);

$new_sort_1=$sorta_1;
$zufall_1_1=$new_sort_1[0]; 
$zufall_1_2=$new_sort_1[1];
$zufall_1_3=$new_sort_1[2];
$zufall_1_4=$new_sort_1[3];
$zufall_1_5=$new_sort_1[4];


foreach ($array2 as $txt5[]){}
$sorta_2=array();
$sorta_2[0]=$txt5[5];
$sorta_3[0]=$txt5[6];
sort($sorta_2);
$new_sort2_1=$sorta_2;
$new_sort2_2=$sorta_3;
$a=$new_sort2_1[0][0];
$b=$new_sort2_2[0][0];


$sortierung=array($a,$b);
sort($sortierung);
$zufall_sub_1_1=$sortierung[0]; 
$zufall_sub_1_2=$sortierung[1]; 



}


