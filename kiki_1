$inhalt = fread ($handle, filesize ($dateinamen));
fclose ($handle);

$inhalt ="no";

 
$handle = fopen ('./text/feld_freeze_info_3.txt', 'w');
fwrite ($handle, $inhalt);
fclose ($handle);


echo'<meta http-equiv="refresh" content="0; URL=http://'.$_SERVER['HTTP_HOST'].$_SERVER['REQUEST_URI'].'">';	
	} 
	
